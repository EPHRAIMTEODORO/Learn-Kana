// ============================================================================
// UTILITY FUNCTIONS
// ============================================================================

/**
 * Get all kanji as a flat array
 */
export const getAllKanji = (): KanjiCharacter[] => {
  return kanjiGradeSections.flatMap(section => section.kanji);
};

/**
 * Get kanji by grade
 */
export const getKanjiByGrade = (grade: string): KanjiCharacter[] => {
  const section = kanjiGradeSections.find(s => s.grade === grade);
  return section ? section.kanji : [];
};

/**
 * Get kanji by multiple grades
 */
export const getKanjiByGrades = (grades: string[]): KanjiCharacter[] => {
  return kanjiGradeSections
    .filter(section => grades.includes(section.grade))
    .flatMap(section => section.kanji);
};

/**
 * Search kanji by character
 */
export const findKanjiByCharacter = (character: string): KanjiCharacter | undefined => {
  return getAllKanji().find(k => k.character === character);
};

/**
 * Get total kanji count
 */
export const getTotalKanjiCount = (): number => {
  return kanjiGradeSections.reduce((sum, section) => sum + section.totalCount, 0);
};

/**
 * Get loaded kanji count (currently in dataset)
 */
export const getLoadedKanjiCount = (): number => {
  return getAllKanji().length;
};
